{
  "name": "nuxt-baseshop",
  "version": "0.0.1",
  "private": false,
  "description": "VELUX Online Sales pplication utilizing Sitecore JavaScript Services and Vue (vue-cli).",
  "author": {
    "name": "VELUX",
    "url": "https://velux.com"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "/*.js",
    "/*.d.ts"
  ],
  "scripts": {
    "start:connected": "npm-run-all --serial bootstrap nuxt:dev start:watch-components",
    "start:production": "npm-run-all --serial bootstrap nuxt:build nuxt:start",
    "bootstrap": "ts-node --require dotenv-flow/config --project tsconfig.scripts.json scripts/bootstrap.ts",
    "start:vue": "vue-cli-service serve --open --host localhost",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build": "cross-env NODE_ENV=production npm-run-all --serial bootstrap nuxt:build",
    "nuxt:build": "nuxi build",
    "nuxt:dev": " nuxi dev",
    "dev": "nuxi dev",
    "nuxt:start": "nuxi start",
    "nuxt:generate": "nuxi generate",
    "nuxt:preview": "nuxi preview",
    "nuxt:postinstall": "nuxi prepare",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest watch"
  },
  "config": {
    "appName": "baseshop",
    "buildArtifactsPath": "./dist",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreApiKey": "{35537F26-6B0A-4A4F-8B76-02D823E4A4FE}",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "sitecoreDistPath": "/dist/baseshop",
    "templates": [
      "vue"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@headlessui/vue": "^1.7.16",
    "@nuxtjs/i18n": "^8.2.0",
    "@optimize-lodash/rollup-plugin": "^4.0.4",
    "@pinia/nuxt": "^0.5.1",
