    "@types/url-parse": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.29.2",
